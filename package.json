{
  "name": "vibebench",
  "version": "1.0.0",
  "description": "CLI tool for VibeBench - vote on AI models from your terminal",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "vibebench": "bin/vibebench.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "lint": "echo 'Linting not configured yet'",
    "clean": "rm -rf dist/"
  },
  "keywords": ["ai", "models", "voting", "cli", "terminal", "vibebench", "artificial-intelligence", "leaderboard", "command-line"],
  "author": "Denis Kolbas",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "node-fetch": "^3.3.0",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/denis-kolbas/vibebench-cli.git"
  },
  "homepage": "https://vibebench.io",
  "bugs": {
    "url": "https://github.com/denis-kolbas/vibebench-cli/issues"
